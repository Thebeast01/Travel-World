{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "build" : {
      "env" : {
        "REACT_APP_API_ENDPOINT": "https://travel-world-gamma.vercel.app"
      }
  }
}
